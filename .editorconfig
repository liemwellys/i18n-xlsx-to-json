# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

# Tab indentation
[*]
indent_style = space
indent_size = 4
trim_trailing_whitespace = true
insert_final_newline = true

# Markdown files
[*.md]
indent_size = 2

# i18n JSON files output
[i18n-output/**/*.json]
indent_size = 2
